<?xml version="1.0" ?>
<robot name="mobile_robot_with_arm">
  <!-- ===================== Mobile Base ==============================================-->
  <!--車体リンク-->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.8 0.5 0.15"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.8 0.5 0.15"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="150.0" />
        <inertia ixx="3.4" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="8.3" 
            iyz="0"
            izx="0"  izy="0"  izz="11.1" />
    </inertial>
  </link>

  <!-- 左車輪リンク  -->
  <link name="left_wheel_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="3.0" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0002" />
    </inertial>
  </link>

  <!-- 車体リンク <-> 左車輪リンク をつなげるジョイント (Rear Left) -->
  <joint name="left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel_link"/>
    <origin xyz="-0.3 -0.27 -0.075" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- 右車輪リンク  -->
  <link name="right_wheel_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="3.0" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0002" />
    </inertial>
  </link>

  <!-- 車体リンク <-> 右車輪リンク をつなげるジョイント (Rear Right) -->
  <joint name="right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="right_wheel_link"/>
    <origin xyz="-0.3 0.27 -0.075" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- 前左車輪リンク  -->
  <link name="front_left_wheel_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="3.0" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0002" />
    </inertial>
  </link>

  <!-- 車体リンク <-> 前左車輪リンク をつなげるジョイント -->
  <joint name="front_left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_left_wheel_link"/>
    <origin xyz="0.3 -0.27 -0.075" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- 前右車輪リンク  -->
  <link name="front_right_wheel_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="3.0" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0002" />
    </inertial>
  </link>

  <!-- 車体リンク <-> 前右車輪リンク をつなげるジョイント -->
  <joint name="front_right_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="front_right_wheel_link"/>
    <origin xyz="0.3 0.27 -0.075" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- 前キャスターリンク  -->
  <link name="front_caster_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <box size="0.08 0.08 0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <box size="0.08 0.08 0.08"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="5.0" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0002" />
    </inertial>
  </link>

  <!-- 前キャスタージョイント  -->
  <joint name="front_caster_joint" type="fixed">
    <parent link="base_link"/>
    <child link="front_caster_link"/>
    <origin xyz="0.35 0.0 -0.11" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- 後キャスターリンク  -->
  <link name="back_caster_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <box size="0.08 0.08 0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <box size="0.08 0.08 0.08"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="5.0" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0002" />
    </inertial>
  </link>

  <!-- 後キャスタージョイント  -->
  <joint name="back_caster_joint" type="fixed">
    <parent link="base_link"/>
    <child link="back_caster_link"/>
    <origin xyz="-0.35 0.0 -0.11" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!--バンパー-->
  <link name="bumper_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0 "/>
      <geometry>
        <box size="0.015 0.5 0.15"/>
      </geometry>
      <material name="white">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0 "/>
      <geometry>
        <box size="0.015 0.5 0.15"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="0.0001" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0002" />
    </inertial>
  </link>

  <!--バンパージョイント-->
  <joint name="bumper_joint" type="fixed">
    <parent link="base_link"/>
    <child link="bumper_link"/>
    <origin xyz="0.4075 0.0 0.0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- ===================== 2D Lidar ==============================================-->
  <!--lidarリンク-->
  <link name="lidar_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0 "/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
      <material name="red">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0 "/>
      <geometry>
        <cylinder radius="0.03" length="0.05"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="0.5" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0001" />
    </inertial>
  </link>

  <!--lidarジョイント-->
  <joint name="lidar_joint" type="fixed">
    <parent link="base_link"/>
    <child link="lidar_link"/>
    <origin xyz="0.0 0.0 0.0975" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- ===================== RGB-D Camera ==============================================-->
  <!-- RGB-D Camera Link -->
  <link name="rgbd_camera_link">
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.12 0.05"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.08 0.12 0.05"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="0.2" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0001" />
    </inertial>
  </link>

  <!-- RGB-D Camera Joint -->
  <joint name="rgbd_camera_joint" type="fixed">
    <parent link="base_link"/>
    <child link="rgbd_camera_link"/>
    <origin xyz="0.4 0.0 0.12" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!--pybulletでカメラの目標地点として設定する用の仮想的なlink-->
  <link name="rgbd_camera_target_vertual_link">
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="0.0001" />
        <inertia ixx="0.000034533333" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.000034533333" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0000648" />
    </inertial>
  </link>

  <joint name="rgbd_camera_target_joint" type="fixed">
    <parent link="rgbd_camera_link"/>
    <child link="rgbd_camera_target_vertual_link"/>
    <origin xyz="0.2 0.0 0.0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- ===================== Arm Base Mount ==============================================-->
  <!-- Arm base mount link (cylinder to mount the arm on the mobile base) -->
  <link name="arm_base_mount_link">
    <visual>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.2"/>
      </geometry>
      <material name="gray">
        <color rgba="0.5 0.5 0.5 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0.1" rpy="0 0 0"/>
      <geometry>
        <cylinder radius="0.12" length="0.2"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="2.0" />
        <inertia ixx="0.01" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.01" 
            iyz="0"
            izx="0"  izy="0"  izz="0.01" />
    </inertial>
  </link>

  <!-- Arm base mount joint -->
  <joint name="arm_base_mount_joint" type="fixed">
    <parent link="base_link"/>
    <child link="arm_base_mount_link"/>
    <origin xyz="0.0 0.0 0.075" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
  </joint>

  <!-- ===================== UR5 Arm ==============================================-->
  <!-- UR5 Materials -->
  <material name="LightGrey">
    <color rgba="0.8 0.8 0.8 1.0"/>
  </material>
  <material name="DarkGrey">
    <color rgba="0.2 0.2 0.2 1.0"/>
  </material>

  <!-- UR5 Base Link -->
  <link name="ur5_base_link">
    <visual>
      <geometry>
        <mesh filename="ur5/visual/base.stl"/>
      </geometry>
      <material name="LightGrey"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="ur5/collision/base.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="4.0"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.00443333156" ixy="0.0" ixz="0.0" iyy="0.00443333156" iyz="0.0" izz="0.0072"/>
    </inertial>
  </link>

  <!-- Joint connecting UR5 base to mount -->
  <joint name="ur5_base_mount_joint" type="fixed">
    <parent link="arm_base_mount_link"/>
    <child link="ur5_base_link"/>
    <origin xyz="0.0 0.0 0.2" rpy="0 0 0"/>
  </joint>

  <!-- UR5 Shoulder Pan Joint -->
  <joint name="shoulder_pan_joint" type="revolute">
    <parent link="ur5_base_link"/>
    <child link="shoulder_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.089159"/>
    <axis xyz="0 0 1"/>
    <limit effort="150.0" lower="-6.28318530718" upper="6.28318530718" velocity="3.15"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- UR5 Shoulder Link -->
  <link name="shoulder_link">
    <visual>
      <geometry>
        <mesh filename="ur5/visual/shoulder.stl"/>
      </geometry>
      <material name="DarkGrey"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="ur5/collision/shoulder.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="3.7"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.010267495893" ixy="0.0" ixz="0.0" iyy="0.010267495893" iyz="0.0" izz="0.00666"/>
    </inertial>
  </link>

  <!-- UR5 Shoulder Lift Joint -->
  <joint name="shoulder_lift_joint" type="revolute">
    <parent link="shoulder_link"/>
    <child link="upper_arm_link"/>
    <origin rpy="0.0 1.57079632679 0.0" xyz="0.0 0.13585 0.0"/>
    <axis xyz="0 1 0"/>
    <limit effort="150.0" lower="-6.28318530718" upper="6.28318530718" velocity="3.15"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- UR5 Upper Arm Link -->
  <link name="upper_arm_link">
    <visual>
      <geometry>
        <mesh filename="ur5/visual/upperarm.stl"/>
      </geometry>
      <material name="LightGrey"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="ur5/collision/upperarm.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="8.393"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.28"/>
      <inertia ixx="0.22689067591" ixy="0.0" ixz="0.0" iyy="0.22689067591" iyz="0.0" izz="0.0151074"/>
    </inertial>
  </link>

  <!-- UR5 Elbow Joint -->
  <joint name="elbow_joint" type="revolute">
    <parent link="upper_arm_link"/>
    <child link="forearm_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 -0.1197 0.425"/>
    <axis xyz="0 1 0"/>
    <limit effort="150.0" lower="-3.14159265359" upper="3.14159265359" velocity="3.15"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- UR5 Forearm Link -->
  <link name="forearm_link">
    <visual>
      <geometry>
        <mesh filename="ur5/visual/forearm.stl"/>
      </geometry>
      <material name="DarkGrey"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="ur5/collision/forearm.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="2.275"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.25"/>
      <inertia ixx="0.049443313556" ixy="0.0" ixz="0.0" iyy="0.049443313556" iyz="0.0" izz="0.004095"/>
    </inertial>
  </link>

  <!-- UR5 Wrist 1 Joint -->
  <joint name="wrist_1_joint" type="revolute">
    <parent link="forearm_link"/>
    <child link="wrist_1_link"/>
    <origin rpy="0.0 1.57079632679 0.0" xyz="0.0 0.0 0.39225"/>
    <axis xyz="0 1 0"/>
    <limit effort="28.0" lower="-6.28318530718" upper="6.28318530718" velocity="3.2"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- UR5 Wrist 1 Link -->
  <link name="wrist_1_link">
    <visual>
      <geometry>
        <mesh filename="ur5/visual/wrist1.stl"/>
      </geometry>
      <material name="LightGrey"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="ur5/collision/wrist1.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.219"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.111172755531" ixy="0.0" ixz="0.0" iyy="0.111172755531" iyz="0.0" izz="0.21942"/>
    </inertial>
  </link>

  <!-- UR5 Wrist 2 Joint -->
  <joint name="wrist_2_joint" type="revolute">
    <parent link="wrist_1_link"/>
    <child link="wrist_2_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.093 0.0"/>
    <axis xyz="0 0 1"/>
    <limit effort="28.0" lower="-6.28318530718" upper="6.28318530718" velocity="3.2"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- UR5 Wrist 2 Link -->
  <link name="wrist_2_link">
    <visual>
      <geometry>
        <mesh filename="ur5/visual/wrist2.stl"/>
      </geometry>
      <material name="DarkGrey"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="ur5/collision/wrist2.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="1.219"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.111172755531" ixy="0.0" ixz="0.0" iyy="0.111172755531" iyz="0.0" izz="0.21942"/>
    </inertial>
  </link>

  <!-- UR5 Wrist 3 Joint -->
  <joint name="wrist_3_joint" type="revolute">
    <parent link="wrist_2_link"/>
    <child link="wrist_3_link"/>
    <origin rpy="0.0 0.0 0.0" xyz="0.0 0.0 0.09465"/>
    <axis xyz="0 1 0"/>
    <limit effort="28.0" lower="-6.28318530718" upper="6.28318530718" velocity="3.2"/>
    <dynamics damping="0.0" friction="0.0"/>
  </joint>

  <!-- UR5 Wrist 3 Link (End Effector) -->
  <link name="wrist_3_link">
    <visual>
      <geometry>
        <mesh filename="ur5/visual/wrist3.stl"/>
      </geometry>
      <material name="LightGrey"/>
    </visual>
    <collision>
      <geometry>
        <mesh filename="ur5/collision/wrist3.stl"/>
      </geometry>
    </collision>
    <inertial>
      <mass value="0.1879"/>
      <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
      <inertia ixx="0.0171364731454" ixy="0.0" ixz="0.0" iyy="0.0171364731454" iyz="0.0" izz="0.033822"/>
    </inertial>
  </link>

  <!-- ===================== Robotiq Gripper ==============================================-->
  <!-- Gripper Material -->
  <material name="Blanc">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>

  <!-- Gripper Base Link (connects to UR5 wrist_3_link) -->
  <link name="gripper_base_link">
    <inertial>
      <mass value="0.001"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
  </link>

  <!-- Joint connecting gripper to UR5 wrist_3_link -->
  <joint name="wrist_3_gripper_joint" type="fixed">
    <parent link="wrist_3_link"/>
    <child link="gripper_base_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.0"/>
  </joint>

  <!-- Robotiq Base -->
  <joint name="gripper_base_link_robotiq_2f_85_base_joint" type="fixed">
    <parent link="gripper_base_link"/>
    <child link="robotiq_2f_85_base"/>
    <origin rpy="0 0 0" xyz="0 0 0"/>
  </joint>
  <link name="robotiq_2f_85_base">
    <inertial>
