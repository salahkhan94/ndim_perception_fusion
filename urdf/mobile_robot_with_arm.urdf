<?xml version="1.0" ?>
<robot name="mobile_robot_with_arm">
  <!-- ===================== Mobile Base ==============================================-->
  <!--車体リンク-->
  <link name="base_link">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 0.15"/>
      </geometry>
      <material name="blue">
        <color rgba="0 0 1 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <box size="0.5 0.5 0.15"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="15.0" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0002" />
    </inertial>
  </link>

  <!-- 左車輪リンク  -->
  <link name="left_wheel_link">
    <visual>
      <origin xyz="0 0 0" rpy="1.57 0 0 "/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
      <material name="black">
        <color rgba="0 0 0 1"/>
      </material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="1.57 0 0"/>
      <geometry>
        <cylinder length="0.04" radius="0.08"/>
      </geometry>
    </collision>
    <inertial>
        <origin xyz="0 0 0" rpy="0 0 0" />
        <mass value="3.0" />
        <inertia ixx="0.0001" 
            ixy="0"  ixz="0"
            iyx="0"  iyy="0.0001" 
            iyz="0"
            izx="0"  izy="0"  izz="0.0002" />
    </inertial>
  </link>

  <!-- 車体リンク <-> 左車輪リンク をつなげるジョイント -->
  <joint name="left_wheel_joint" type="continuous">
    <parent link="base_link"/>
    <child link="left_wheel_link"/>
    <origin xyz="0.0 -0.27 -0.075" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
  </joint>

  <!-- 右車輪リンク  -->
  <link name="right_wheel_link">
